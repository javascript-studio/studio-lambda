{
  "name": "@studio/lambda",
  "version": "4.0.0",
  "description": "JavaScript Studio lambda execution environment",
  "author": "Maximilian Antoni <max@javascript.studio>",
  "homepage": "https://github.com/javascript-studio/studio-lambda",
  "main": "lib/lambda.js",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "lint": "eslint .",
    "test": "mocha --slow=500",
    "watch": "npm run test -- --watch",
